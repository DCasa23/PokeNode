
<%- include("template/cabecera", {cabecera: 'Nuevo Entrenador' }) %>
<div class="container">
    <h1>Crear nuevo Entrenador</h1>
    <form action="/entrenador" class="my-2" method="POST">
        <input type="text" placeholder="Nombre" class="form-control my-2" name="nombre">
        <input type="text" placeholder="Tipo Pokemon" class="form-control my-2" name="tipo">
        <input type="text" placeholder="Nivel" class="form-control my-2" name="nivel">
        <input type="text" placeholder="Nivel Amistad" class="form-control my-2" name="amistad">
        
        <button type="submit" class="btn btn-dark btn-block">
            Agregar
        </button>
    </form>
</div>
<%- include("template/footer") %>

